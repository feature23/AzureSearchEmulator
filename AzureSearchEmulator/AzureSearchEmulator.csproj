	</ItemGroup>

	<ItemGroup>
	  <Folder Include="indexes\" />
	</ItemGroup>

	<ItemGroup>
	  <None Update="aspnetapp.pfx">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </None>
	</ItemGroup>

</Project>
